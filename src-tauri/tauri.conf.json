{
  "productName": "liturgy-of-the-day",
  "version": "0.0.0",
  "identifier": "com.tauri.dev",
  "build": {
    "beforeDevCommand": "trunk serve",
    "devUrl": "http://localhost:1777",
    "beforeBuildCommand": "trunk build",
    "frontendDist": "../csr/dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "🐑📖🙏 Liturgia da Palavra ✝️✨🕊️",
        "width": 1440,
        "height": 900,
        "fullscreen": false,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
